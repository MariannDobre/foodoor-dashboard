# Supabase project config
SUPABASE_URL=your-supabase-project-url
SUPABASE_KEY=your-supabase-anon-key

# Auth.js (NextAuth.js) config
AUTH_URL=your-desired-localhost   # or your deployed domain
AUTH_SECRET=your-auth-secret

# Social provider keys (e.g. Google login)
AUTH_GOOGLE_ID=your-google-client-id
AUTH_GOOGLE_SECRET=your-google-client-secret

# Public base URL (used for redirects, e.g. after auth)
NEXT_PUBLIC_BASE_URL=your-desired-localhost

